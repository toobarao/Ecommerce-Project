@model Shipping

<div class="container">
    <form method="post" >
   <table class="table">
       <tr>
           <th>Contact</th>
            <td>@Model.Contact</td>
            <td><button>Change</button></td>
       </tr>
        <tr>
            <th>Ship To</th>
            <td>@Model.ShipTo</td>
            <td><button>Change</button></td>
        </tr>

   </table>
   <h1>Shipping Method</h1>
   <p>@Model.ShippingMethod</p>
        <button type="button" class="btn btn-primary">Return To Information</button>

        <button type="submit" class="btn btn-primary">Continue Payment</button>
    </form>
</div>

